<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.selfroom.mappers.RoomDataMapper">
    <select id="getRoomData" resultType="com.example.selfroom.bean.RoomData">
        SELECT * FROM test.roominformation  WHERE floor = #{floor}
    </select>

    <select id="getAllRoomData" resultType="com.example.selfroom.bean.RoomData">
        SELECT * FROM test.roominformation
    </select>
</mapper>